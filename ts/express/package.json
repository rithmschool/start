{
  "name": "rithmschool-start-ts-express",
  "version": "1.0.0",
  "homepage": "https://github.com/rithmschool/start/",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node $npm_package_main",
    "start:debug": "node --inspect-brk $npm_package_main",
    "test": "vitest ts",
    "test:cov": "vitest ts --coverage",
    "test:debug": "vitest -t *.ts --run --inspect-brk --no-file-parallelism"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "@types/supertest": "^6.0.2",
    "supertest": "^6.3.4",
    "vitest": "^1.2.2"
  },
  "dependencies": {
    "express": "^5.0.0-beta.1"
  }
}
